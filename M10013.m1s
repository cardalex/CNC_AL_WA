'Corresponding button "Meas Z Offset"rem #define DIGITIZE     22Dim cycleNumDim currZOffset As Double Code "G90 G21 S20000 G17 F10"			'slow feed rate to 10 MM/MIN	For cycleNum = 1 To 200	If IsActive(DIGITIZE) = False Then		currZOffset = GetOemDro(802)					nextZoffset = currZOffset - 0.01					Code "G0 Z"&nextZoffset				While IsMoving()			Sleep(100)		Wend						currZOffset = nFmt(GetOemDro(802), 4)		SetVar(2002,currZOffset)		End IfNext cycleNumMessage "Z Offset = " &currZOffset If IsActive(DIGITIZE) = False Then	Speak"Z Contact not reached"Else	Speak"Z Contact Done"	Speak"Offset is "& nFmt(currZOffset,4)		End If	